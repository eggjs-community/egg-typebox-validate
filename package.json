{
  "name": "egg-typebox-validate-fengmk2",
  "version": "3.0.0-beta.1",
  "description": "another validate for typescript egg projects",
  "eggPlugin": {
    "name": "typeboxValidate",
    "exports": {
      "import": "./dist/esm",
      "require": "./dist/commonjs",
      "typescript": "./src"
    }
  },
  "keywords": [
    "egg",
    "eggjs",
    "validate",
    "ajv",
    "typebox",
    "eggPlugin",
    "egg-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xiekw2010/egg-typebox-validate.git"
  },
  "author": "xiekw2010",
  "engines": {
    "node": ">= 18.19.0"
  },
  "dependencies": {
    "@eggjs/core": "^6.2.13",
    "@sinclair/typebox": "^0.23.0",
    "ajv": "^8.8.2",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@eggjs/bin": "7",
    "@eggjs/mock": "^6.0.5",
    "@eggjs/tsconfig": "1",
    "@types/mocha": "10",
    "@types/node": "22",
    "@types/semver": "^7.5.8",
    "benchmark": "^2.1.4",
    "egg": "^4.0.8",
    "egg-logger": "^3.6.1",
    "eslint": "8",
    "eslint-config-egg": "14",
    "glob": "^11.0.1",
    "parameter": "^3.7.0",
    "rimraf": "6",
    "semver": "^7.7.1",
    "snap-shot-it": "^7.9.10",
    "supertest": "^6.3.3",
    "tshy": "3",
    "tshy-after": "1",
    "typescript": "5"
  },
  "scripts": {
    "benchmark": "node ./benchmark/ajv-vs-parameter.mjs",
    "lint": "eslint --cache src test --ext .ts",
    "pretest": "npm run clean && npm run lint -- --fix",
    "test": "egg-bin test",
    "test:snapshot:update": "SNAPSHOT_UPDATE=1 egg-bin test",
    "preci": "npm run clean &&  npm run lint",
    "ci": "egg-bin cov",
    "postci": "npm run prepublishOnly && npm run clean",
    "clean": "rimraf dist",
    "prepublishOnly": "tshy && tshy-after && attw --pack --profile node16"
  },
  "type": "module",
  "tshy": {
    "exports": {
      ".": "./src/index.ts",
      "./decorator": "./src/decorator.ts",
      "./typebox": "./src/typebox.ts",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./decorator": {
      "import": {
        "types": "./dist/esm/decorator.d.ts",
        "default": "./dist/esm/decorator.js"
      },
      "require": {
        "types": "./dist/commonjs/decorator.d.ts",
        "default": "./dist/commonjs/decorator.js"
      }
    },
    "./typebox": {
      "import": {
        "types": "./dist/esm/typebox.d.ts",
        "default": "./dist/esm/typebox.js"
      },
      "require": {
        "types": "./dist/commonjs/typebox.d.ts",
        "default": "./dist/commonjs/typebox.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/commonjs/index.d.ts",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js"
}
